# OpenAI configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# Gemini configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash

# FreeFlow Python service configuration
FREEFLOW_SERVICE_URL=http://localhost:8001

# Aviationstack (flight status)
AVIATIONSTACK_API_KEY=your_aviationstack_api_key_here
AVIATIONSTACK_API_BASE_URL=http://api.aviationstack.com/v1

# Open-Meteo (weather, no API key required)
OPEN_METEO_BASE_URL=https://api.open-meteo.com/v1

# Default LLM provider: gemini | openai | mock | freeflow
LLM_PROVIDER=freeflow

PORT=3000
